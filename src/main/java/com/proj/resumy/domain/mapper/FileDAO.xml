<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.proj.resumy.file.domain.FileDAO">

	<select id="selectByMid" resultType="com.proj.resumy.file.domain.FileDTO">
		SELECT 
			file_name, 
			file_volume, 
			file_regdate, 
			file_memo 
		FROM 
			hr_file
		WHERE mem_id = #{mid}
		ORDER BY 
			file_regdate DESC;
	</select>
</mapper>